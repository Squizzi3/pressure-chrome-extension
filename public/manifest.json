{
  "name": "Pressure",
  "description": "Pressure Chrome Extension by TheBrettWay and Kingsnetic",
  "version": "0.0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://opensea.io/*"
      ],
      "js": [
        "./static/js/PressureApp.js"
      ],
      "css": [
        "/css/PressureApp.css"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/css/PressureApp.css",
        "/images/*"
      ],
      "matches": [
        "https://opensea.io/*"
      ]
    }
  ],
  "action": {
    "default_icon": {
      "16": "/images/logo_16x16.png",
      "32": "/images/logo_32x32.png"
    }
  },
  "icons": {
    "16": "/images/logo_16x16.png",
    "32": "/images/logo_32x32.png",
    "48": "/images/logo_48x48.png",
    "128": "/images/logo_128x128.png"
  },
  "host_permissions": [
    "http://localhost:3000/",
    "http://localhost:3001/",
    "http://localhost:5000/",
    "http://localhost:8080/",
    "https://opensea.io/*"
  ],
  "background": {
    "service_worker": "./static/js/background.js",
    "type": "module"
  },
  "permissions": [
    "webRequest",
    "storage",
    "cookies",
    "tabs",
    "scripting"
  ]
}